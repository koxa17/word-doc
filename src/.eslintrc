{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node":true
    },
    "rules": {
        "semi" : "off" ,
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "indent": "off",
        "eol-last":"off",
        "object-curly-spacing": "off",
        "comma-dangle": "off",
        "arrow-parens": "off"
    },
    "extends": ["eslint:recommended", "google"]
}